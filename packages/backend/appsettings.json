{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=singleclin-prod-postgres.postgres.database.azure.com;Database=singleclin;Username=singleclinadmin;Password=SingleClin123!;Port=5432;SSL Mode=Require;",
    "AzureCommunication": "endpoint=https://singleclin-communication.unitedstates.communication.azure.com/;accesskey=BQY89zDLtokhtBTkue99G63ezKThU2uDhk61A4tTgENwg40QmhHHJQQJ99BIACULyCpKFlWvAAAAAZCSCaST"
  },
  "Firebase": {
    "ProjectId": "singleclin-app",
    "ServiceAccountKeyPath": "firebase-service-account.json",
    "CloudMessaging": {
      "ServerKey": "",
      "SenderId": "",
      "EnableLogging": true,
      "TimeoutSeconds": 30
    },
    "Authentication": {
      "Issuer": "https://securetoken.google.com/singleclin-app",
      "Audience": "singleclin-app",
      "TokenExpiration": "24:00:00"
    }
  },
  "JWT": {
    "Issuer": "https://localhost:5001",
    "Audience": "SingleClin.API",
    "SecretKey": "YOUR_JWT_SECRET_KEY_HERE",
    "AccessTokenExpirationInMinutes": 120,
    "RefreshTokenExpirationInDays": 7
  },
  "Redis": {
    "ConnectionString": "localhost:6379",
    "InstanceName": "SingleClin",
    "DefaultExpirationMinutes": 30,
    "EnableConnectionMultiplexer": true
  },
  "RateLimit": {
    "QRValidation": {
      "DefaultLimit": 100,
      "WindowMinutes": 1,
      "Clinics": {
        "example-clinic-id": {
          "Limit": 200,
          "WindowMinutes": 1
        }
      }
    }
  },
  "SendGrid": {
    "ApiKey": "",
    "DefaultFromEmail": "noreply@singleclin.com",
    "DefaultFromName": "SingleClin",
    "EnableClickTracking": true,
    "EnableOpenTracking": true,
    "TimeoutSeconds": 30
  },
  "Notifications": {
    "EnablePush": true,
    "EnableEmail": true,
    "DefaultRetryAttempts": 3,
    "RetryDelaySeconds": 5,
    "QuietHours": {
      "Start": "22:00",
      "End": "08:00"
    },
    "LowBalanceThresholds": [3, 2, 1]
  },
  "AzureStorage": {
    "ConnectionString": "YOUR_AZURE_STORAGE_CONNECTION_STRING_HERE",
    "ContainerName": "clinic-images",
    "BaseUrl": "https://singleclin.blob.core.windows.net/",
    "MaxFileSize": 5242880,
    "AllowedFileTypes": ["jpg", "jpeg", "png", "webp"],
    "ImageQuality": 85,
    "MaxWidth": 1200,
    "MaxHeight": 800,
    "SasToken": {
      "DefaultReadExpiryHours": 24,
      "DefaultReadWriteExpiryHours": 1,
      "CacheDurationMinutes": 30,
      "EnableCaching": true,
      "MaxCacheSize": 1000
    }
  },
  "DefaultAdmin": {
    "Email": "admin@example.com",
    "Password": "Admin123!"
  },
  "AzureCommunication": {
    "DefaultFromEmail": "DoNotReply@2bd7ddec-fe47-419c-ac5f-b337e3a81e02.azurecomm.net",
    "DefaultFromName": "SingleClin",
    "EnableClickTracking": false,
    "EnableOpenTracking": false,
    "TimeoutSeconds": 30
  }
}
