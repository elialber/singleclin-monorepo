{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=singleclin_dev;Username=app_user;Password=dev_password_123"
  },
  "Firebase": {
    "ProjectId": "",
    "ServiceAccountPath": "",
    "CloudMessaging": {
      "ServerKey": "",
      "SenderId": "",
      "EnableLogging": true,
      "TimeoutSeconds": 30
    }
  },
  "JWT": {
    "Issuer": "https://localhost:5001",
    "Audience": "SingleClin.API",
    "SecretKey": "ThisIsADevelopmentSecretKeyThatShouldBeChangedInProduction123!",
    "AccessTokenExpirationInMinutes": 15,
    "RefreshTokenExpirationInDays": 7
  },
  "Redis": {
    "ConnectionString": "localhost:6379",
    "InstanceName": "SingleClin",
    "DefaultExpirationMinutes": 30,
    "EnableConnectionMultiplexer": true
  },
  "RateLimit": {
    "QRValidation": {
      "DefaultLimit": 100,
      "WindowMinutes": 1,
      "Clinics": {
        "example-clinic-id": {
          "Limit": 200,
          "WindowMinutes": 1
        }
      }
    }
  },
  "SendGrid": {
    "ApiKey": "",
    "DefaultFromEmail": "noreply@singleclin.com",
    "DefaultFromName": "SingleClin",
    "EnableClickTracking": true,
    "EnableOpenTracking": true,
    "TimeoutSeconds": 30
  },
  "Notifications": {
    "EnablePush": true,
    "EnableEmail": true,
    "DefaultRetryAttempts": 3,
    "RetryDelaySeconds": 5,
    "QuietHours": {
      "Start": "22:00",
      "End": "08:00"
    },
    "LowBalanceThresholds": [3, 2, 1]
  }
}
