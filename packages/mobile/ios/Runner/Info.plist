			<array>
				<string>singleclin</string>
			</array>
		</dict>
		<dict>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>com.googleusercontent.apps.410494012909-pn2m3j632metfv56li6q17il8r12jt9q</string>
			</array>
		</dict>
	</array>
