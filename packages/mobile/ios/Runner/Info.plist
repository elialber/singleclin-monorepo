	<key>NSMotionUsageDescription</key>
	<string>Este aplicativo pode usar dados de movimento para recursos de saúde e fitness.</string>
	
	<!-- Contacts (if social features are implemented) -->
	<key>NSContactsUsageDescription</key>
	<string>Este aplicativo pode acessar seus contatos para recursos sociais.</string>
	
	<!-- Calendar (for appointments) -->
	<key>NSCalendarsUsageDescription</key>
	<string>Este aplicativo pode adicionar compromissos ao seu calendário.</string>
	
